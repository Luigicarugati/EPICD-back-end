@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Totale Verbali per Trasgressore";
}

<h1>Totale Verbali per Trasgressore</h1>

<table class="table">
    <thead>
        <tr>
            <th>Trasgressore</th>
            <th>Totale Verbali</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Trasgressore</td>
                <td>@item.TotaleVerbali</td>
            </tr>
        }
    </tbody>
</table>
